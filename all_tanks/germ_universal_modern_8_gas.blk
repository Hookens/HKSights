drawLines{
  //= = around -O-
  line{
    line:p4=2.5, 2, 7.5, 2
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=2.5, -2, 7.5, -2
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=-2.5, 2, -7.5, 2
    move:b=no
	  thousandth:b = yes
  } 
  line{
    line:p4=-2.5, -2, -7.5, -2
    move:b=no
	  thousandth:b = yes
  }

  //- - around =-O-=
  line{
    line:p4=-2.5, 0, -5, 0
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=2.5, 0, 5, 0
    move:b=no
	  thousandth:b = yes
  }

  //. . around -=-O-=-
  line{
    line:p4=-7.4, 0, -7.5, 0
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=7.4, 0, 7.5, 0
    move:b=no
	  thousandth:b = yes
  }

  //- - around .-=-O-=-.
  line{
    line:p4=-12.5, 0, -10, 0
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=12.5, 0, 10, 0
    move:b=no
	  thousandth:b = yes
  }

  //- - around O
  line{
    line:p4=0.25, 0, 1, 0
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=-0.25, 0, -1, 0
    move:b=no
	  thousandth:b = yes
  }

  //| over O
  line{
    line:p4=0, -0.25, 0, -1
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=0, 0.25, 0, 1
    move:b=no
	  thousandth:b = yes
  }

  //| over |
  line{
    line:p4=0, -2, 0, -4
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=0, 2, 0, 4
    move:b=no
	  thousandth:b = yes
  }

  //| over ||
  line{
    line:p4=0, -6, 0, -8
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=0, 6, 0, 8
    move:b=no
	  thousandth:b = yes
  }


  //zero line
  line{
    line:p4=-23.4, -3.6, -21.7, -3.6
    move:b=yes
	  thousandth:b = yes
  }

  //zero arrow
  line{
    line:p4=-21.4, -3.6, -20.8, -4
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=-21.4, -3.6, -20.8, -3.2
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=-20.8, -4, -20.8, -3.2
    move:b=no
	  thousandth:b = yes
  }
}

