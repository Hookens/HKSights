drawLines{
  //= = around -O-
  line{
    line:p4=2.5, 2, 7.5, 2
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=2.5, -2, 7.5, -2
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=-2.5, 2, -7.5, 2
    move:b=no
	  thousandth:b = yes
  } 
  line{
    line:p4=-2.5, -2, -7.5, -2
    move:b=no
	  thousandth:b = yes
  }

  //- - around =-O-=
  line{
    line:p4=-2.5, 0, -5, 0
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=2.5, 0, 5, 0
    move:b=no
	  thousandth:b = yes
  }

  //. . around -=-O-=-
  line{
    line:p4=-7.4, 0, -7.5, 0
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=7.4, 0, 7.5, 0
    move:b=no
	  thousandth:b = yes
  }

  //- - around .-=-O-=-.
  line{
    line:p4=-12.5, 0, -10, 0
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=12.5, 0, 10, 0
    move:b=no
	  thousandth:b = yes
  }

  //- - around O
  line{
    line:p4=0.25, 0, 1, 0
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=-0.25, 0, -1, 0
    move:b=no
	  thousandth:b = yes
  }

  //| over O
  line{
    line:p4=0, -0.25, 0, -1
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=0, 0.25, 0, 1
    move:b=no
	  thousandth:b = yes
  }

  //| over |
  line{
    line:p4=0, -2, 0, -4
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=0, 2, 0, 4
    move:b=no
	  thousandth:b = yes
  }

  //| over ||
  line{
    line:p4=0, -6, 0, -8
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=0, 6, 0, 8
    move:b=no
	  thousandth:b = yes
  }


  //zero line
  line{
    line:p4=-18, -3.6, -16.7, -3.6
    move:b=yes
	  thousandth:b = yes
  }

  //zero arrow
  line{
    line:p4=-16.5, -3.6, -16, -3.85
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=-16.5, -3.6, -16, -3.35
    move:b=no
	  thousandth:b = yes
  }
  line{
    line:p4=-16, -3.85, -16, -3.35
    move:b=no
	  thousandth:b = yes
  }
}

